# Android fstab file.
#<src>                                                  <mnt_point>         <type>    <mnt_flags>                                                                         <fs_mgr_flags>
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK


/dev/block/mmcblk0p1           /system             ext4      ro                                                    wait
/dev/block/mmcblk0p2           /cache              ext4      rw,nosuid,nodev,noatime,user_xattr,acl,barrier=1,journal_async_commit,nodelalloc,data=writeback    wait
/dev/block/mmcblk0p7           /data               ext4      rw,nosuid,nodev,noatime,user_xattr,acl,barrier=1,journal_async_commit,nodelalloc,data=writeback    wait

# VOLD mount points
/devices/platform/sdhci-tegra.0/mmc_host/mmc2 /storage/sdcard1  auto default voldmanaged=sdcard:auto
/devices/platform/tegra-ehci.2/usb1/1-1/1-1.2 /storage/usbdisk0 auto default voldmanaged=sda:auto
/devices/platform/tegra-ehci.2/usb2/2-1/2-1.2 /storage/usbdisk0 auto default voldmanaged=sda:auto
/devices/platform/tegra-ehci.2/usb1/1-1/1-1.1 /storage/usbdisk1 auto default voldmanaged=sdb:auto
/devices/platform/tegra-ehci.2/usb2/2-1/2-1.1 /storage/usbdisk1 auto default voldmanaged=sdb:auto
/devices/platform/tegra-ehci.2/usb2/2-1/2-1:1 /storage/usbdisk1 auto default voldmanaged=sdb:auto
/devices/platform/tegra-ehci.2/usb1/1-1/1-1:1 /storage/usbdisk1 auto default voldmanaged=sdb:auto
/devices/platform/tegra-ehci.2/usb1/1-1/1-1.3 /storage/sdcard2  auto default voldmanaged=sdcard:auto
/devices/platform/tegra-ehci.2/usb2/2-1/2-1.3 /storage/sdcard2  auto default voldmanaged=sdcard:auto
